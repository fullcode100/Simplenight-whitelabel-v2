{
  "name": "simplenight-whitelabel",
  "private": true,
  "scripts": {
    "dev": "export NEXT_PUBLIC_LAST_COMMIT_HASH=$(git rev-parse --short HEAD) && export NEXT_PUBLIC_LAST_COMMIT_DATE=$(git --no-pager show -s --format=\"%ci\" HEAD) && next dev",
    "dev:up": "docker-compose -f docker/docker-compose.yaml up -d --build",
    "dev:down": "docker-compose -f docker/docker-compose.yaml down",
    "build": "export NEXT_PUBLIC_LAST_COMMIT_HASH=$(git rev-parse --short HEAD) && export NEXT_PUBLIC_LAST_COMMIT_DATE=$(git --no-pager show -s --format=\"%ci\" HEAD) && next build",
    "start": "export NEXT_PUBLIC_LAST_COMMIT_HASH=$(git rev-parse --short HEAD) && export NEXT_PUBLIC_LAST_COMMIT_DATE=$(git --no-pager show -s --format=\"%ci\" HEAD) && next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "styleguide": "TAILWIND_MODE=watch styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "@tailwindcss/forms": "^0.4.0",
    "antd": "^4.18.2",
    "axios": "^0.24.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "dayjs": "^1.10.7",
    "i18next": "^21.6.14",
    "lodash": "^4.17.21",
    "next": "12.1.0",
    "next-react-svg": "^1.1.3",
    "query-string": "^7.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dynamic-import": "^1.1.5",
    "react-i18next": "^11.15.7",
    "react-redux": "^7.2.6",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "sass": "^1.45.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/preset-react": "^7.16.7",
    "@svgr/webpack": "^6.2.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.6.0",
    "eslint": "8.5.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.0",
    "prettier": "^2.5.1",
    "react-docgen-typescript": "^2.2.2",
    "react-styleguidist": "^11.2.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.12",
    "ts-loader": "^9.2.6",
    "typescript": "4.5.4"
  }
